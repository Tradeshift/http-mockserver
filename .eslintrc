{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": "standard",
  "rules": {
    "prefer-arrow-callback": 1,
    "prefer-const": 2,
    "no-var": 2,
    "semi": [2, "always"],
    "no-tabs": 0,
    "indent": [
      2,
      "tab",
      {
        "SwitchCase": 1
      }
    ]
  }
}
